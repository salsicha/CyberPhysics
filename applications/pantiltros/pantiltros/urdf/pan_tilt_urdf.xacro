  <collision>
    <origin xyz="-0.0001099 0.0164388 -0.0939786" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://pan_tilt_ros/meshes/payload_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="base_inertial_fixed" type="fixed">
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="base_link_inertial"/>
</joint>

<joint name="base_tilt_fixed" type="fixed">
  <origin xyz="0.01243 -0.016155 0.0275" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="tilt_motor"/>
</joint>

<joint name="tilt_joint" type="continuous">
  <origin xyz="-0.03275 0.0337 0.01025" rpy="0 0 0"/>
  <parent link="tilt_motor"/>
  <child link="pan_tilt_bracket"/>
  <axis xyz="-0.0 1.0 0.0"/>
</joint>

<joint name="pan_joint" type="continuous">
  <origin xyz="0.0 -0.017125 0.0363" rpy="0 0 0"/>
  <parent link="pan_tilt_bracket"/>
  <child link="pan_motor"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

<joint name="payload_mount_fixed" type="fixed">
  <origin xyz="0.0204299 -0.0168588 0.0199286" rpy="0 0 0"/>
  <parent link="pan_motor"/>
  <child link="payload_link"/>
</joint>

</robot>
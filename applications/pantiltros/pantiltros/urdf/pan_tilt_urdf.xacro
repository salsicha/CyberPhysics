  <visual>
    <origin xyz="0.02032 -0.00042 -0.07405" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://pan_tilt_ros/meshes/pan_motor.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="matte_black"/>
  </visual>
  <collision>
    <origin xyz="0.02032 -0.00042 -0.07405" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://pan_tilt_ros/meshes/pan_motor.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="payload_link">
  <inertial>
    <origin xyz="-0.004991360468491857 -0.011542339627033346 0.004227044433613547" rpy="0 0 0"/>
    <mass value="0.09449139072993021"/>
    <inertia ixx="2.1e-05" iyy="1.8e-05" izz="2.1e-05" ixy="-6e-06" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.0001099 0.0164388 -0.0939786" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://pan_tilt_ros/meshes/payload_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="matte_black"/>
